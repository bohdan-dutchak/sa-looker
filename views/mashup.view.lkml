                  WHEN LOWER(rdp2.brand) LIKE '%acnemoist%' THEN 'Acnemoist'
                  WHEN LOWER(rdp2.brand) LIKE '%acnesta%' THEN 'Actestar'
                  WHEN LOWER(rdp2.brand) LIKE '%acnesti%' THEN 'Acnestil'
