                  WHEN LOWER(rdp2.brand) LIKE '%mama%' THEN 'Mamaearth'
                  WHEN LOWER(rdp2.brand) LIKE '%apollo %' THEN 'Apollo Cosmetics'
                  WHEN LOWER(rdp2.brand) LIKE '%basiclab%' THEN 'BasicLab'
                  WHEN LOWER(rdp2.brand) LIKE '%body works%' OR LOWER(rdp2.brand) LIKE '%bodywio%' THEN 'Bath & Body Works'


            END
      ORDER BY
        3 DESC
      ;;
  }

