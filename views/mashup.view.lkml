                  WHEN LOWER(rdp2.brand) LIKE '%acqua alle rosse%' THEN 'Acqua Alle Rosse'
                  WHEN LOWER(rdp2.brand) LIKE '%adapal%' THEN 'Adapalene Gel'
                  WHEN LOWER(rdp2.brand) LIKE '%a derma%' OR LOWER(rdp2.brand) LIKE '%a-derma%' THEN 'A-DERMA LABORATOIRE DERMATOLOGIQUE VEGETAL'
                  WHEN LOWER(rdp2.brand) LIKE '%acnemyci%' OR LOWER(rdp2.brand) LIKE '%acne-myci%' THEN 'Acne-Mycin'
                  WHEN LOWER(rdp2.brand) LIKE '%anixa%' THEN 'anixa Life Sciences'
                  WHEN LOWER(rdp2.brand) LIKE '%skinceuticals%' THEN 'SkinCeuticals'
                  WHEN LOWER(rdp2.brand) LIKE '%anixa%' THEN 'anixa Life Sciences'
                  WHEN LOWER(rdp2.brand) LIKE '%drunk elephant%' THEN 'Drunk Elephant'
                  WHEN LOWER(rdp2.brand) LIKE '%olay%' THEN 'Olay'
                  WHEN LOWER(rdp2.brand) LIKE '%cetaphil%' THEN 'Cetaphil'
                  WHEN LOWER(rdp2.brand) LIKE '%clinique%' THEN 'Clinique'
                  WHEN LOWER(rdp2.brand) LIKE '%tatcha%' THEN 'Tatcha'
                  WHEN LOWER(rdp2.brand) LIKE '%aveeno%' THEN 'Aveeno'
                  WHEN LOWER(rdp2.brand) LIKE '%clinique%' THEN 'Clinique'
                  WHEN LOWER(rdp2.brand) LIKE '%herbivore%' THEN 'Herbivore'
